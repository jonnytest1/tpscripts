    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "attach",
            "name": "Attach to Remote",
            "address": "127.0.0.1",
            "port": 19365,
            "localRoot": "${workspaceFolder}",
            "remoteRoot": "${workspaceFolder}"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "program": "${workspaceFolder}\\index.js",
            "autoAttachChildProcesses": true,
            "restart": true,
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch NodeTS",
            "program": "D:\\vm\\dockervm\\storage\\tpscripts\\pi\\bas\\time\\src\\init.ts",
            "autoAttachChildProcesses": true,
            "restart": true,
        },
        {
            "type": "php",
            "request": "launch",
            "name": "debug php on pi",
            "log": true,
            "pathMappings": {
                "/var/www/html/": "d:\\vm\\dockervm\\storage\\tpscripts\\pi\\tampermonkey\\"
            }
        }
    ]
}